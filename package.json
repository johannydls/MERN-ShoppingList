{
  "name": "mern-stack-yt",
  "version": "1.0.0",
  "description": "Shopping list built with the MERN Stack",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && yarn install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "yarn --cwd client start",
    "client-npm": "npm start --prefix client",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
  },
  "author": "Johanny LS",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "express": "^4.17.1",
    "mongoose": "^5.7.3"
  },
  "devDependencies": {
    "nodemon": "^1.19.3"
  }
}
